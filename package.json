    "@rvf/zod": "7.0.3",
    "@sanity/client": "7.3.0",
    "@tailwindcss/vite": "4.1.7",
    "canvas-confetti": "1.9.3",
    "classnames": "2.5.1",
    "date-fns": "4.1.0",
