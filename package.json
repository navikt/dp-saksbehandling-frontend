{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "start": "remix-serve ./build/server/index.js",
    "type-check": "tsc --project tsconfig.json",
    "prepare": "husky install",
    "playwright": "npx playwright test",
    "playwright:ui": "npx playwright test --ui",
    "playwright:verbose": "DEBUG=pw:api npx playwright test",
    "test": "vitest",
    "test-coverage": "vitest --coverage",
    "generate-token": "chmod +x ./token-generator.sh && ./token-generator.sh",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "prettier": {
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --list-different",
      "eslint --max-warnings 0",
      "bash -c 'npm run type-check'"
    ]
  },
  "dependencies": {
    "@elastic/ecs-pino-format": "^1.5.0",
    "@grafana/faro-web-sdk": "^1.7.0",
    "@grafana/faro-web-tracing": "^1.7.0",
    "@navikt/aksel-icons": "^6.6.1",
    "@navikt/ds-css": "^6.6.1",
    "@navikt/ds-react": "^6.6.1",
    "@navikt/oasis": "^3.2.3",
    "@opentelemetry/sdk-node": "^0.51.0",
    "@remix-run/express": "^2.9.1",
    "@remix-run/node": "^2.9.1",
    "@remix-run/react": "^2.9.1",
    "@remix-run/serve": "^2.9.1",
    "@remix-validated-form/with-zod": "^2.0.7",
    "classnames": "^2.5.1",
    "date-fns": "^3.6.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "groq": "^3.39.1",
    "isbot": "^5.1.5",
    "pino": "^9.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remix-validated-form": "^5.1.5",
    "tiny-invariant": "^1.3.3",
    "tinyduration": "^3.3.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "^4.2.5",
    "@graphql-codegen/typescript": "4.0.6",
    "@parcel/watcher": "^2.4.1",
    "@playwright/test": "^1.43.1",
    "@portabletext/types": "^2.0.13",
    "@remix-run/dev": "^2.9.1",
    "@remix-run/eslint-config": "^2.9.1",
    "@remix-run/testing": "^2.9.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.5",
    "@testing-library/user-event": "^14.5.2",
    "@types/compression": "^1.7.5",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-istanbul": "^1.5.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "msw": "^2.2.14",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.2"
  },
  "engines": {
    "node": ">=20"
  },
  "optionalDependencies": {
    "@esbuild/linux-x64": "^0.20.0",
    "@rollup/rollup-linux-x64-gnu": "^4.9.6"
  }
}
