{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "start": "remix-serve ./build/server/index.js",
    "type-check": "tsc --project tsconfig.json",
    "prepare": "husky",
    "playwright": "npx playwright test",
    "playwright:ui": "npx playwright test --ui",
    "playwright:verbose": "DEBUG=pw:api npx playwright test",
    "test": "vitest",
    "test-coverage": "vitest --coverage",
    "generate-token": "chmod +x ./token-generator.sh && ./token-generator.sh",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "prettier": {
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --list-different",
      "eslint --max-warnings 0",
      "bash -c 'npm run type-check'"
    ]
  },
  "dependencies": {
    "@elastic/ecs-pino-format": "1.5.0",
    "@grafana/faro-web-sdk": "1.12.0",
    "@grafana/faro-web-tracing": "1.12.0",
    "@navikt/aksel-icons": "7.5.1",
    "@navikt/ds-css": "7.5.1",
    "@navikt/ds-react": "7.5.1",
    "@navikt/oasis": "3.4.0",
    "@opentelemetry/sdk-node": "0.55.0",
    "@portabletext/react": "3.1.0",
    "@portabletext/to-html": "2.0.13",
    "@remix-run/node": "2.14.0",
    "@remix-run/react": "2.14.0",
    "@remix-run/serve": "2.14.0",
    "@rvf/remix": "^6.3.0",
    "@rvf/zod": "^6.0.3",
    "@sanity/client": "6.22.5",
    "canvas-confetti": "1.9.3",
    "classnames": "2.5.1",
    "date-fns": "4.1.0",
    "graphql": "16.9.0",
    "graphql-request": "7.1.2",
    "groq": "3.64.1",
    "isbot": "5.1.17",
    "pino": "9.5.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "remix-utils": "7.7.0",
    "tiny-invariant": "1.3.3",
    "tinyduration": "3.3.1",
    "unleash-client": "6.1.3",
    "uuid": "11.0.3",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/client-preset": "4.5.0",
    "@graphql-codegen/typescript": "4.1.1",
    "@parcel/watcher": "2.5.0",
    "@playwright/test": "1.49.0",
    "@portabletext/types": "2.0.13",
    "@remix-run/dev": "2.14.0",
    "@remix-run/eslint-config": "2.14.0",
    "@remix-run/testing": "2.14.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "14.5.2",
    "@types/canvas-confetti": "1.6.4",
    "@types/compression": "1.7.5",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/uuid": "10.0.0",
    "@vitejs/plugin-react": "4.3.3",
    "@vitest/coverage-istanbul": "2.1.5",
    "eslint": "8.57.1",
