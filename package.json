    "graphql-request": "7.1.2",
    "groq": "3.76.1",
    "isbot": "5.1.23",
    "lru-cache": "11.0.2",
    "pino": "9.6.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "remix-utils": "7.7.0",
