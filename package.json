{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build && run-p build:server",
    "build:server": "tsc --project server/tsconfig.json",
    "dev": "remix dev",
    "start": "ts-node ./server/server.ts",
    "type-check": "tsc --project tsconfig.json",
    "prepare": "husky install",
    "playwright": "npx playwright test",
    "playwright:ui": "npx playwright test --ui",
    "playwright:verbose": "DEBUG=pw:api npx playwright test",
    "test": "vitest",
    "test-coverage": "vitest --coverage",
    "generate-token": "chmod +x ./token-generator.sh && ./token-generator.sh",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "prettier": {
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --list-different",
      "eslint --max-warnings 0",
      "bash -c 'npm run type-check'"
    ]
  },
  "dependencies": {
    "@grafana/faro-web-sdk": "^1.3.9",
    "@grafana/faro-web-tracing": "^1.3.9",
    "@navikt/aksel-icons": "^6.1.0",
    "@navikt/ds-css": "^6.1.0",
    "@navikt/ds-react": "^6.1.0",
    "@navikt/oasis": "^1.0.2",
    "@remix-run/css-bundle": "^2.7.2",
    "@remix-run/express": "^2.7.2",
    "@remix-run/node": "^2.7.2",
    "@remix-run/react": "^2.7.2",
    "@remix-run/serve": "^2.7.2",
    "@remix-validated-form/with-zod": "^2.0.7",
    "classnames": "^2.5.1",
    "date-fns": "^3.3.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "groq": "^3.30.0",
    "prom-client": "^15.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-validated-form": "^5.1.5",
    "tiny-invariant": "^1.3.1",
    "tinyduration": "^3.3.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "^4.2.3",
    "@graphql-codegen/typescript": "4.0.5",
    "@parcel/watcher": "^2.4.0",
    "@playwright/test": "^1.41.2",
    "@portabletext/types": "^2.0.8",
    "@remix-run/dev": "^2.7.2",
    "@remix-run/eslint-config": "^2.7.2",
    "@remix-run/testing": "^2.7.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-istanbul": "^1.3.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "msw": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=20"
  },
  "optionalDependencies": {
    "@esbuild/linux-x64": "^0.20.0",
    "@rollup/rollup-linux-x64-gnu": "^4.9.6"
  }
}
