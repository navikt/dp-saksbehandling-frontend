{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build && run-p build:server",
    "build:server": "tsc --project server/tsconfig.json",
    "dev": "remix dev",
    "start": "ts-node ./server/server.ts",
    "type-check": "tsc --project tsconfig.json",
    "prepare": "husky install",
    "playwright": "npx playwright test",
    "playwright:ui": "npx playwright test --ui",
    "playwright:verbose": "DEBUG=pw:api npx playwright test",
    "test": "vitest",
    "test-coverage": "vitest --coverage",
    "generate-token": "chmod +x ./token-generator.sh && ./token-generator.sh",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "prettier": {
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --list-different",
      "eslint --max-warnings 0",
      "bash -c 'npm run type-check'"
    ]
  },
  "dependencies": {
    "@grafana/faro-web-sdk": "^1.3.1",
    "@grafana/faro-web-tracing": "^1.3.1",
    "@navikt/aksel-icons": "^5.12.2",
    "@navikt/dp-auth": "^0.3.15",
    "@navikt/ds-css": "^5.12.2",
    "@navikt/ds-react": "^5.12.2",
    "@portabletext/types": "^2.0.8",
    "@remix-run/css-bundle": "^2.3.1",
    "@remix-run/express": "^2.1.0",
    "@remix-run/node": "^2.1.0",
    "@remix-run/react": "^2.1.0",
    "@remix-run/serve": "^2.3.1",
    "@remix-validated-form/with-zod": "^2.0.7",
    "@sanity/client": "^6.9.1",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "groq": "^3.21.0",
    "husky": "^8.0.3",
    "i18n-iso-countries": "^7.7.0",
    "isbot": "^3.7.1",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "prom-client": "^14.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-validated-form": "^5.1.5",
    "tiny-invariant": "^1.3.1",
    "tinyduration": "^3.3.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@parcel/watcher": "^2.3.0",
    "@playwright/test": "^1.40.1",
    "@remix-run/dev": "^2.3.1",
    "@remix-run/eslint-config": "^2.3.1",
    "@remix-run/testing": "^2.3.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-istanbul": "^0.34.6",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "jsdom": "^22.1.0",
    "msw": "^2.0.13",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=20"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.9.6"
  }
}
