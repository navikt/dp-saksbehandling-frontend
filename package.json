    "@portabletext/to-html": "2.0.14",
    "@remix-run/node": "2.15.3",
    "@remix-run/react": "2.15.3",
    "@remix-run/serve": "2.15.3",
    "@rvf/remix": "6.3.0",
    "@rvf/zod": "6.0.3",
    "canvas-confetti": "1.9.3",
