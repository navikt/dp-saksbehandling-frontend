{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build && run-p build:server",
    "build:server": "tsc --project server/tsconfig.json",
    "dev": "remix dev",
    "start": "ts-node ./server/server.ts",
    "type-check": "tsc",
    "prepare": "husky install",
    "playwright": "npx playwright test",
    "playwright:ui": "npx playwright test --ui",
    "test": "vitest",
    "test-coverage": "vitest --coverage"
  },
  "prettier": {
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --list-different",
      "eslint --max-warnings 0"
    ]
  },
  "dependencies": {
    "@navikt/aksel-icons": "^4.7.2",
    "@navikt/dp-auth": "^0.3.3",
    "@navikt/ds-css": "^4.7.2",
    "@navikt/ds-css-internal": "^3.4.3",
    "@navikt/ds-react": "^4.7.2",
    "@navikt/ds-react-internal": "^3.4.3",
    "@portabletext/types": "^2.0.5",
    "@remix-run/css-bundle": "^1.19.1",
    "@remix-run/node": "^1.19.1",
    "@remix-run/react": "^1.19.1",
    "@remix-run/serve": "^1.19.1",
    "@remix-validated-form/with-zod": "^2.0.6",
    "@sanity/client": "^6.1.7",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "graphql-request": "^6.1.0",
    "groq": "^3.14.4",
    "husky": "^8.0.3",
    "i18n-iso-countries": "^7.6.0",
    "isbot": "^3.6.13",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "prom-client": "^14.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-quill": "^2.0.0",
    "remix-utils": "^6.6.0",
    "remix-validated-form": "^5.0.2",
    "tiny-invariant": "^1.3.1",
    "tinyduration": "^3.3.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.36.2",
    "@remix-run/dev": "^1.19.1",
    "@remix-run/eslint-config": "^1.19.1",
    "@types/express": "^4.17.17",
    "@types/react": "^18.2.16",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.2",
    "@vitest/coverage-v8": "^0.34.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "msw": "^1.2.3",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vitest": "^0.34.0"
  },
  "engines": {
    "node": ">=14"
  }
}
